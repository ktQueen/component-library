    "infoDataConvert":{
        "dataType":"function",
        "value":"function(data){\nif (data.id) {\n                    data.department = {\n                        \"id\": data.departmentId,\n                        \"name\": data.userDepart\n                    };\n                    data.orderType = {\n                        \"id\": data.orderTypeId,\n                        \"name\": data.orderTypeName\n                    };\n\n                    if (data.opOrderClusterOfflineDtos) {\n                        var opOrderClusterOfflineDtosArr = [];\n                        for (var i = 0; i < data.opOrderClusterOfflineDtos.length; i++) {\n                            var dots = data.opOrderClusterOfflineDtos[i];\n                            dots.offline = {\n                                id: dots.offlineCode,\n                                name: dots.offlineName\n                            }\n                            opOrderClusterOfflineDtosArr.push(dots);\n                        }\n                        data.opOrderClusterOfflineDtos = opOrderClusterOfflineDtosArr;\n\n                    }\n                    return data;\n                } else {\n                    return {\n                        department: {\n                            id: data.departmentId,\n                            name: data.userDepart,\n                        }\n                    }\n                }\n}"
    },
    "selectDataConvert":{
        "dataType":"function",
        "value":null
    },
    "saveDataConvert":{
        "dataType":"function",
        "value":"function(data,info){\n var opOrderClusterOfflineDtos = []; //集群下线信息\n                $.each(data.opOrderClusterOfflineDtos, function(i, v) {\n                    opOrderClusterOfflineDtos.push({\n                        \"clusterDomain\": v.clusterDomain, //集群对应的域名\n                        \"clusterName\": v.clusterName, //集群名称\n                        \"indexNum\": v.indexNum, //序号\n                        \"offlineCode\": v.offline.id, //域名是否下线code\n                        \"offlineName\": v.offline.name //域名是否下线名称\n                    });\n                });\n                return {\n                    \"id\": data.id,\n                    \"applyReason\": data.applyReason, //申请原因\n                    \"userId\": info.userId, //申请人id\n                    \"chineseName\": info.chineseName, //申请人名称\n                    \"departmentId\": info.departmentId, //申请人部门id\n                    \"userDepart\": info.userDepart, //申请人部门名称\n                    \"orderTypeId\": info.orderType.id, //申请类型id\n                    \"orderTypeName\": info.orderType.name, //申请类型名称\n                    \"opOrderClusterOfflineDtos\": opOrderClusterOfflineDtos\n                }\n}"
    }
