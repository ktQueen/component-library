    "infoDataConvert":{
        "dataType":"function",
        "value":"function (info, data) {\n        var formData = {};\n        formData.formId = info.baseForm.formId;\n        formData.moneyType = { id: 0, name: \"人民币\" }; //币种\n        formData.empDepartment = info.baseForm.empDepartment; //部门\n        formData.costCenter = info.baseForm.costCenter;//成本中心\n        //编辑\n        if (info.baseForm.id && info.baseForm.id) {\n            formData.planRepaymentDate=info.baseForm.planRepaymentDate;//预计还款时间\n            var reg = new RegExp(\"
\", \"g\");\n            formData.costDesc = info.baseForm.costDesc.replace(reg, \"\\n\"); //借款原因\n            formData.innerRemark = info.baseForm.innerRemark.replace(reg, \"\\n\"); //内部备注\n            formData.fileList = info.fileList; //文件上传\n            formData.supplementMoney = info.baseForm.applyMoney + '元'; //合计金额\n            if (info.costDetails && info.costDetails.length > 0) {//费用明细\n                var costDetailTypeObj = {};\n                if (data.costDetailType && data.costDetailType.length > 0) {\n                    for (var i = 0; i < data.costDetailType.length; i++) {\n                        var cdt = data.costDetailType[i];\n                        costDetailTypeObj[cdt.code] = cdt\n                    }\n                }\n                formData.costDetail = [];\n                for (var i = 0; i < info.costDetails.length; i++) {\n                    var costDetail = info.costDetails[i];\n                    formData.costDetail[i] = {\n                        \"amount\": costDetail.amount,\n                        \"costDetailType\": costDetail.costDetailType ? {\n                            id: costDetail.costDetailType,\n                            name: costDetailTypeObj[costDetail.costDetailType].name + \"(\" + costDetailTypeObj[costDetail.costDetailType].code + \")\"\n                        } : null,\n                        \"costdetaildi\": 3,\n                        \"costHappenDate\": costDetail.costHappenDate\n                    }\n                }\n            }\n            if (info.approveHistory && info.approveHistory.length > 0) {//审批历史\n                formData.approveHistory = []; //费用明细\n                var optionObj = {\n                    1: \"发起流程\",\n                    2: \"审批通过\",\n                    3: \"流程加签\",\n                    4: \"撤回流程\",\n                    5: \"驳回流程\",\n                    6: \"流程跳过\",\n                    7: \"任务认领\",\n                    8: \"暂存待办\",\n                    9: \"回复\"\n                }\n                for (var i = 0; i < info.approveHistory.length; i++) {\n                    var approveHistory = info.approveHistory[i];\n                    formData.approveHistory[i] = {\n                        \"assigneeName\": approveHistory.assigneeName, //金额\n                        \"suggestion\": approveHistory.suggestion,\n                        \"thisOptionStr\": optionObj[approveHistory.thisOption],\n                        \"approveTimeStr\": new Date(approveHistory.approveTime).toLocaleString()\n                    }\n                }\n            }\n        }\n        return formData;\n    }"
},
"selectDataConvert":{
"dataType":"function",
"value":"function (data) {\n        var cdtTemp = [];\n        for (var i = 0, l = data.costDetailType.length; i < l; i++) {\n            cdtTemp.push({\n                id: data.costDetailType[i].code,\n                name: data.costDetailType[i].name + \"(\" + data.costDetailType[i].code + \")\"\n            })\n        }\n        return {\n            costDetail: [{\n                costDetailType: cdtTemp//费用明细类别\n            }]\n        };\n    }"
},
"saveDataConvert":{
"dataType":"function",
"value":"function (info, data) {\n        var costDetailTemp = [];\n        $.each(info.costDetail, function(i, v) {\n            costDetailTemp.push({\n                \"amount\": v.amount ? v.amount : 0,\n                \"costDetailType\": v.costDetailType.id,\n                \"costdetaildiv\": 3,\n                \"costHappenDate\": v.costHappenDate.startTime ? v.costHappenDate.startTime.substring(0, 10) : null\n            });\n        });\n        return {\n            baseForm: JSON.stringify({\n                formId: info.formId,\n                moneyType: info.moneyType.id,\n                costDesc: info.costDesc.replace(/\\n|\\r\\n/g, \"
\"),\n                innerRemark: info.innerRemark.replace(/\\n|\\r\\n/g, \"
\"),\n                applyMoney: info.applyMoney.replace(\"元\", \"\") ? info.applyMoney.replace(\"元\", \"\") : 0,\n                planRepaymentDate:info.planRepaymentDate.startTime//预计还款时间\n            }),\n            costDetails: JSON.stringify(costDetailTemp ? costDetailTemp : []),\n            fileList: JSON.stringify(info.fileList ? info.fileList : [])\n        }\n    }"
}
